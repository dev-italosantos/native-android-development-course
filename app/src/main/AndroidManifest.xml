<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.monetario">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Monetario">
        <activity
            android:name=".view.course.alcoholOrGasoline.AlcoholOrGasoline"
            android:exported="false" />
        <activity
            android:name=".view.ijon.Login"
            android:exported="true" />
        <activity
            android:name=".view.course.jokenpo.Jokenpo"
            android:exported="false" />
        <activity
            android:name=".view.course.phrase.PhraseDay"
            android:exported="true"
            android:screenOrientation="sensor" />
        <activity
            android:name=".view.course.netfilx.Netflix"
            android:exported="true"
            android:screenOrientation="sensor" />
        <activity
            android:name=".view.coronation.Coronation"
            android:exported="true" />
        <activity
            android:name=".view.iuse.Home"
            android:exported="true" />
        <activity
            android:name=".view.ijon.Register"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".view.ijon.Beginning"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>